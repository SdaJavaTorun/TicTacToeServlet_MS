<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>HelloWorldFantastic</servlet-name>
    <servlet-class>pl.sdaacademy.HelloWorldFantastic</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TicTacToe</servlet-name>
    <servlet-class>pl.sdaacademy.TicTacToeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloWorldFantastic</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TicTacToe</servlet-name>
    <url-pattern>/put</url-pattern>
  </servlet-mapping>

</web-app>


